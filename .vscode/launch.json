{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "serve",
      "program": "${workspaceFolder}/node_modules/.bin/vue-cli-service",
      "args": ["serve"],
      "env": {
        "VUE_CLI_SERVICE_CONFIG_PATH": "${workspaceFolder}/vue.config.js"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "icons",
      "program": "${workspaceFolder}/scripts/iconfont-to-scss.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "snippet",
      "program": "${workspaceFolder}/scripts/scss-to-snippet.js",
      "console": "integratedTerminal"
    }
  ]
}
